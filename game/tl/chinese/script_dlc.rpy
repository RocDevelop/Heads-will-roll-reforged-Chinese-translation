translate chinese blacksmith_talk_0_ccf8b1f5:

    # bl "{cps=50}Если там есть, что лапать, конечно.{vspace=5}Потому что видел я его девку — плоская как доска."
    bl ""

# game/script_dlc.rpy:42516
translate chinese blacksmith_talk_0_22b5e866:

    # bl "{cps=50}Хотя на лицо и правда неплохая..."
    bl ""

# game/script_dlc.rpy:42517
translate chinese blacksmith_talk_0_d409b0df:

    # h "{cps=50}У всех свои вкусы."
    h ""

# game/script_dlc.rpy:42518
