    h ""

# game/script_dlc.rpy:15029
translate chinese a_visit_to_the_devil_end_32a74c58:

    # h "{cps=50}Постой, ты же не имеешь в виду..."
    h ""

# game/script_dlc.rpy:15030
translate chinese a_visit_to_the_devil_end_bf01b535:

    # b "{cps=50}Да."
    b ""

# game/script_dlc.rpy:15031
translate chinese a_visit_to_the_devil_end_7a41c5c9:

    # b "{cps=50}Мы выбираемся из Камбрэ на рассвете, во время развода караула. Я уже обо всем договорился.{w} А дальше...{w} Ну а дальше уже как получится."
    b ""

# game/script_dlc.rpy:15032
translate chinese a_visit_to_the_devil_end_7a6ba84b:

    # h "{cps=50}Ты не планируешь возвращаться, да?"
    h ""

# game/script_dlc.rpy:15033
translate chinese a_visit_to_the_devil_end_9990a8bd_1:

    # b "{cps=50}..."
    b ""

# game/script_dlc.rpy:15034
