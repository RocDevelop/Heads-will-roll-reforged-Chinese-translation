translate chinese a_visit_to_the_devil_ruffin_296da51e:

    # nvl clear
    # "{cps=50}Голос одного из них вы узнаёте сразу же."
    nvl clear
    ""

# game/script_dlc.rpy:14444
translate chinese a_visit_to_the_devil_ruffin_e20d8d17:

    # nvl clear
    # "{cps=50}Руффин."
    nvl clear
    ""

# game/script_dlc.rpy:14447
translate chinese a_visit_to_the_devil_ruffin_bfb91ffd:

    # nvl clear
    # "{cps=50}Голоса вам не знакомы."
    nvl clear
    ""

